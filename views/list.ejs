<%- include('header') %>
<div class="box" id="heading">
  <h1><%=listTitle%></h1>
</div>
<div class="box">
  <% newListItems.forEach((items)=>{ %>
    <form action="/delete" method="post">
      <div class="item">
        <input type="checkbox" name="checkedbox" value="<%= items._id %>" onchange="this.form.submit()">
        <p> <%=items.name%></p>
      </div>
    </form>
  <% }); %>

  <form class="item" action="/" method="post">
    <input type="text" name="toDo" placeholder="New List" autocomplete="off">
    <button type="submit" name="submit" value=<%=listTitle%>>+</button>
  </form>
</div>
<%- include('footer') %>
